{
  "name": "spryker/docker-sdk-generator",
  "description": "Spryker Docker SDK - Generator",
  "license": "MIT",
  "require": {
    "symfony/yaml": "^4.3",
    "twig/twig": "^2.11",
    "spatie/url": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "DeployFileGenerator\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "7.3"
    }
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6",
    "phpunit/phpunit": "^9.5",
    "spryker/code-sniffer": "^0.16.5"
  }
}
